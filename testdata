@{ViewBag.Title = "Shelf";}
@model TaskSetup.Models.Task
<style type="text/css">
    div#rMenu
    {
        position: absolute;
        visibility: hidden;
        top: 0;
        background-color: White;
        text-align: left;
        padding: 2px;
    }
    div#rMenu ul li
    {
        margin: 1px;
        padding: 0 5px;
        cursor: pointer;
        list-style: none outside none;
        background-color: #DFDFDF;
    }
    
    ul.ztree
    {
        margin-top: 2px;
        border: 0.5px solid #617775;
        width: 500px;
        height: 400px;
        overflow-y: scroll;
        overflow-x: auto;
    }
</style>
<script type="text/javascript">
    var flgLocationLoaded = false;
    var flgSectionLoaded = false;
    var flgShelfVarLoaded = false;
    var flgUpcLoaded = false;
    var mstrPOD;

    var IDMark_A = "_a";


    var zTreeLocationObj, zTreeSectionObj, zTreeShelfObj, zTreeUPCObj;
    var arrLocationShelfVar;
    var arrSectionShelfVar;
    var arrShelfShelfVar;
    var arrUPCShelfVar;
    var RCAT = 'ReviewCategory',KCAT='KeyCat';
    var currActiveTab;
    var glbShelfTaskNum = null;
    var setting = {
        view: {
            addDiyDom: addDiyDom
        },
        check: {
            enable: true,
            chkStyle: "checkbox",
            chkboxType: { "Y": "p", "N": "s" },
            chkDisabledInherit: true
        },

        data: {
            simpleData: {
                enable: true
            }
        },
        callback: {
            onCheck: myOnCheck,
            onClick: onClick,
            beforeCheck: myBeforeCheckCallBack,
            onDrag:dragNode,
            beforeDrop: beforeDropNode,
            beforeDrag: beforeDragNode,
        },
        edit: {
            enable: true,
            drag: {
                isMove: true,
                inner: false
            },
            showRemoveBtn: false,
            showRenameBtn: false
        }
    };

    function onClick(event, treeId, treeNode, clickFlag) {
        showHideVarDiv(treeNode);        
    }

    function myOnCheck(event, treeId, treeNode) {
        showHideVarDiv(treeNode);
        
        if ((treeNode.id == 11930) || (treeNode.id == 11960)) {
            
            var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
            var node1 = zTree1.getNodeByTId("trSection_34");
            var node2 = zTree1.getNodeByTId("trSection_35");

            if((!node1.checked) && ((!node2.checked)))
                zTree1.checkNode(treeNode);
        }
    }

    function myBeforeCheckCallBack(treeId, treeNode) {
        if (treeNode.optional != 'Y')
            return false;        
    }
    var sourceNode ;
    var tagegetNode;
   

   function beforeDragNode(treeId, treeNodes)
   {
   
    if((treeId =='trLocation' && treeNodes[0].getParentNode().name == 'Display Location') || (treeId =='trSection' && (treeNodes[0].getParentNode().name == 'Department' || treeNodes[0].getParentNode().name == 'Stocking Location')) || (treeId =='trShelf' && treeNodes[0].getParentNode().name == 'Shelf Type'))
      return true;
    else 
     return  false;
   }

    function dragNode(event, treeId, treeNodes)
    {
     sourceNode = treeNodes[0].getParentNode().name;
    }
    
    function beforeDropNode(treeId, treeNodes, targetNode, moveType) {
    tagegetNode = targetNode.getParentNode().name;
     if(sourceNode != tagegetNode)
     {
      return false;
     }
  }

  var activeSectionMinMax ;
  var activeShelfMinMax;

    function showHideVarDiv(treeNode) {
              
        //For shelf --> Height
        if(treeNode.id == 22700)
        {
          if(treeNode.checked)
          {
              $("#tabDummy").show();
              $("#idLimits").show();
              $("#idChecks").hide();
              $("#txtMin").val(treeNode.taskMin);
              $("#txtMax").val(treeNode.taskMax);
              activeShelfMinMax = treeNode.id;
          }
          else{
            $("#idLimits").hide();
          }
          return;
        }
        
        if(treeNode.id == 11160 || treeNode.id == 14825 || treeNode.id == 15995 || treeNode.id == 15490 || treeNode.id == 16910)
        {
         if(treeNode.checked)
         {
              $("#tabDummy").show();
              $("#idLimits").show();
              $("#idChecks").hide();
              $("#txtMin").val(treeNode.taskMin);
              $("#txtMax").val(treeNode.taskMax);
              activeSectionMinMax = treeNode.id;
         }
         else{
          $("#idLimits").hide();
         }
         return;
        }

        if ((treeNode.id == 200) || (treeNode.id == 10100) || (treeNode.id == 20000) || (treeNode.id == 30100)) {
            $("#tabDummy").show();

            $("#txtMin").val(treeNode.mainMin);
            $("#txtMax").val(treeNode.mainMax);
            $("#txtSoft").val(treeNode.main_soft_check);
            $("#txtHard").val(treeNode.main_hard_check);
            
            if(treeNode.taskMin)
                $("#txtMin").val(treeNode.taskMin);

            if (treeNode.taskMax)
                $("#txtMax").val(treeNode.taskMax);

            if (treeNode.task_soft_check)
                $("#txtSoft").val(treeNode.task_soft_check);

            if (treeNode.task_hard_check)
                $("#txtHard").val(treeNode.task_hard_check);
        }
        else
            $("#tabDummy").hide();

            activeShelfMinMax = '';
            activeSectionMinMax ='';
    }

    function reSetShelfVarLoadFlags() {
        flgLocationLoaded = false;
        flgSectionLoaded = false;
        flgShelfVarLoaded = false;
        flgUpcLoaded = false;
    }

    function setCurrTab(trv) {
        currActiveTab = trv;
    }

    function myBeforeCheckCallBack(treeId, treeNode) {
        if (treeNode.optional != 'Y')
            return false;
    }

    function addDiyDom(treeId, treeNode) {
        if (!treeNode.isParent) return;
        var aObj = $("#" + treeNode.tId + IDMark_A);

        var editStr = "<a id='diyBtn1_" + treeNode.id + "' onclick='checkChilds(" + treeNode.tId + ")'><b>(Select all Child variables)</b></a>";
        aObj.after(editStr);        
    }


    function checkChilds(tid) {
        
        var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
        var node = zTree1.getNodeByTId(tid.id);
        zTree1.checkNode(node);
        //zTree1.selectNode(node);

        var nodes = node.children;
        for (var i = 0, l = nodes.length; i < l; i++) {
            zTree1.checkNode(nodes[i], true, true);
        }
    }

    function showHidePOD(secTree, secNode) {
        //If Location - "Store Wall" is checked then display Section - "Presece of Department" options

        var zTree2 = $.fn.zTree.getZTreeObj("trLocation");
        var node1 = zTree2.getNodeByTId("trLocation_1");

        
        //Take back up of "Presece of Department" node
        if (secNode.children[1].name == "Presence of Department") {
            mstrPOD = secNode.children[1];
        }

        secTree.selectNode(mstrPOD);
        if (node1.children[2].checked == false) {
            nods = secTree.getSelectedNodes();
            secTree.hideNodes(nods);
        }

        if ((node1.children[2].checked == true) && (secNode.children[1].isHidden != "true")) {
            var nodes1 = secTree.getNodesByParam("isHidden", true);
            secTree.showNodes(nodes1);
        }
    }

    var objReviewCategoryDtls = null;
    var objSelectedReviewCatDtls = new Array();
    $(document).ready(function () {
        var grdWidth = screen.width-100;
        //debugger;
        $("#tabShelfGroups").tabs({
            activate: function (event, ui) {
                //var $activeTab = $('#tabs').tabs('option', 'active');

                switch (event.currentTarget.id) {
                    case "idtabLocationGrp":
                        currActiveTab = "trLocation";
                        var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
                        var node = zTree1.getNodeByTId("trLocation_1");
                        showHideVarDiv(node);
                        zTree1.selectNode(node);
                        $("#idLimits").show();
                        break;
                    case "idtabSectionGrp":
                        currActiveTab = "trSection";
                        var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
                        var node = zTree1.getNodeByTId("trSection_1");
                        showHideVarDiv(node);
                        showHidePOD(zTree1, node);
                        zTree1.selectNode(node);
                        $("#idLimits").hide();
                        break;
                    case "idtabShelfGrp":
                        currActiveTab = "trShelf";
                        var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
                        var node = zTree1.getNodeByTId("trShelf_1");
                        showHideVarDiv(node);
                        zTree1.selectNode(node);
                        $("#idLimits").hide();
                        break;
                    case "idtabUpcGrp":
                        currActiveTab = "trUPC";
                        var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
                        var node = zTree1.getNodeByTId("trUPC_1");
                        showHideVarDiv(node);
                        zTree1.selectNode(node);
                        $("#idLimits").hide();
                        break;
                }
            }
        });

        $("#tabDummy").tabs();
        currActiveTab = "trLocation";

        var _taskid = '@Model.task_number';
        var _taskVersion = '@Model.version_code';
        var lockout_id = '@Model.lockout_id';
        glbShelfTaskNum = _taskid;
        $.ajax({
            type: "POST",
            url: '@Url.Content("~/Task/GetTaskShelfVar")',
            dataType: "json",
            data: { taskId: _taskid, grp: 1 },
            async: true,
            traditional: true,
            success: function (data) {
                arrLocationShelfVar = data;
                zTreeLocationObj = $.fn.zTree.init($("#trLocation"), setting, arrLocationShelfVar);

                //Show the limit/checks for location
                var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
                var node = zTree1.getNodeByTId("trLocation_1");
                showHideVarDiv(node);
                zTree1.selectNode(node);

                //Lockout controls when task if finalised or being edited by other user
                if (_taskVersion == 'FNL' || lockout_id != '@ViewData["currentUser"]') {
                    $("table").find("input[type=text]").attr('disabled', 'disabled');

                    zTree1.setChkDisabled(node, true);
                    for(var i =0;i<node.children.length ;i++){
                        zTree1.setChkDisabled(node.children[i], true);
                        DisableChilds(zTree1, node.children[i]);
                    }                    
                }
            }
        });

        $.ajax({
            type: "POST",
            url: '@Url.Content("~/Task/GetTaskShelfVar")',
            dataType: "json",
            data: { taskId: _taskid, grp: 2 },
            async: true,
            traditional: true,
            success: function (data) {
                arrSectionShelfVar = data;
                zTreeSectionObj = $.fn.zTree.init($("#trSection"), setting, arrSectionShelfVar);

                var zTree1 = $.fn.zTree.getZTreeObj("trSection");
                var node = zTree1.getNodeByTId("trSection_1");

                //Lockout controls when task if finalised or being edited by other user
                if (_taskVersion == 'FNL' || lockout_id != '@ViewData["currentUser"]') {
                    $("table").find("input[type=text]").attr('disabled', 'disabled');

                    zTree1.setChkDisabled(node, true);
                    for(var i =0;i<node.children.length ;i++){
                        zTree1.setChkDisabled(node.children[i], true);
                        DisableChilds(zTree1, node.children[i]);
                    }                    
                }
            }
        });

        $.ajax({
            type: "POST",
            url: '@Url.Content("~/Task/GetTaskShelfVar")',
            dataType: "json",
            data: { taskId: _taskid, grp: 3 },
            async: true,
            traditional: true,
            success: function (data) {
                arrShelfShelfVar = data;
                zTreeShelfObj = $.fn.zTree.init($("#trShelf"), setting, arrShelfShelfVar);

                var zTree1 = $.fn.zTree.getZTreeObj("trShelf");
                var node = zTree1.getNodeByTId("trShelf_1");

                //Lockout controls when task if finalised or being edited by other user
                if (_taskVersion == 'FNL' || lockout_id != '@ViewData["currentUser"]') {
                    $("table").find("input[type=text]").attr('disabled', 'disabled');

                    zTree1.setChkDisabled(node, true);
                    for(var i =0;i<node.children.length ;i++){
                        zTree1.setChkDisabled(node.children[i], true);
                        DisableChilds(zTree1, node.children[i]);
                    }                    
                }
            }
        });

        $.ajax({
            type: "POST",
            url: '@Url.Content("~/Task/GetTaskShelfVar")',
            dataType: "json",
            data: { taskId: _taskid, grp: 4 },
            async: true,
            traditional: true,
            success: function (data) {
                arrUPCShelfVar = data;
                zTreeUPCObj = $.fn.zTree.init($("#trUPC"), setting, arrUPCShelfVar);

                var zTree1 = $.fn.zTree.getZTreeObj("trUPC");
                var node = zTree1.getNodeByTId("trUPC_1");

                //Lockout controls when task if finalised or being edited by other user
                if (_taskVersion == 'FNL' || lockout_id != '@ViewData["currentUser"]') {
                    $("table").find("input[type=text]").attr('disabled', 'disabled');

                    zTree1.setChkDisabled(node, true);
                    for(var i =0;i<node.children.length ;i++){
                        zTree1.setChkDisabled(node.children[i], true);
                        DisableChilds(zTree1, node.children[i]);
                    }                    
                }
            }
        });

        function DisableChilds(ztreeObj, parentNd) {         
            if (parentNd.children != null && parentNd.children.length > 0){
                for(var i =0;i<parentNd.children.length ;i++){
                    ztreeObj.setChkDisabled(parentNd.children[i], true);                
                    DisableChilds(ztreeObj, parentNd.children[i]);
                }
            }            
        }

        //Allow only integer values in these controls
        $("#txtMin, #txtMax, #txtSoft, #txtHard").bind('keypress', numbersonly);

        //When lost focus from the Limit / Checks Textboxes, get the values from textbox and put it into the tree objects

        $("#txtMin").change(function (e) {
           
           if(currActiveTab == 'trShelf')
           {
              if(ShelfMinValidation())
              {
               updateValuetoTree();
              }
              else{
                $("#txtMin").focus();
                $("#txtMin").val('');
              }
               return;
           }

           if(currActiveTab == 'trSection')
           {
             if(SectionMinValidation())
             {
             updateValuetoTree();
             }
             else{
             $("#txtMin").val('');
             }

             return;
           }



            if (doShelfValidation()) {
                
                updateValuetoTree();
            }
            else {
                resetTreeVal('txtMin');
                $("#txtMin").focus();
                e.preventDefault();
            }
        });

        $("#txtMax").change(function (e) {

        if(currActiveTab == 'trShelf')
        {
         if(ShelfMaxValidation())
         {
          updateValuetoTree();
         }
         else{
         $("#txtMax").val('');
         }
         return;
        }

          if(currActiveTab == 'trSection')
           {
             if(SectionMaxValidation())
             {
              updateValuetoTree();
             }
             else{
              $("#txtMax").val('');
             }

             return;
           }

            if (doShelfValidation()) {
                updateValuetoTree();
            }
            else {
                resetTreeVal('txtMax');
                $("#txtMax").focus();
                e.preventDefault();
            }
        });

        $("#txtSoft").change(function (e) {
            if (doShelfValidation()) {
                updateValuetoTree();
            }
            else {
                resetTreeVal('txtSoft');
                $("#txtSoft").focus();
                e.preventDefault();
            }
        });

        $("#txtHard").change(function (e) {
            if (doShelfValidation()) {
                updateValuetoTree();
            }
            else {
                resetTreeVal('txtHard');
                $("#txtHard").focus();
                e.preventDefault();
            }
        });

        if (_taskVersion == 'FNL' || lockout_id != '@ViewData["currentUser"]') {
            $("table").find("input[type=text]").attr('disabled', 'disabled');
            $("table").find("input[type=checkbox]").attr('disabled', 'disabled');

            $("#btnSaveAccounting").attr('disabled', 'disabled');
        }

        $('#ShelfPage').accordion({
            heightStyle: "content",
            active: 0,
            collapsible: true
        });
        
        //Get review category details object
         $.ajax({
            type: "POST",
            url: '@Url.Content("~/Task/GetReviewCategoryDetailsForShelf")',
            dataType: "json",
            data: { task_number: _taskid },
            async: false,
            traditional: true,
            success: function (data) {
               
               objReviewCategoryDtls = data.ReviewCategories;
               objSelectedReviewCatDtls = data.CollectionCategories;
            },
            error: function (result) {
                alert('An error occured while getting the review category details for the task');
            }
        });

        var reviewCategories = $.grep(objReviewCategoryDtls,function(n,i){
            if(n.KeycatNumber == null || n.KeycatNumber == 0){
                return n;
            }
        });
        var mainGridPrefix = "s_";
        //Review category grid
        jQuery("#grdCollCatDtls").jqGrid({
            mtype: 'GET',
            data: reviewCategories,
            datatype: 'local',
            async: false,
            colNames: ['ReviewCatNumber', 'Review Category', 'Keycat', 'Keycat Name', 'Selected', 'QCUnder', 'Pass/Fail%', 'Weeks'],
            colModel: [
                    { name: 'ReviewCategoryNumber', index: 'ReviewCategoryNumber', width: 50, sortable: false, hidden: true },
                    { name: 'ReviewCategoryName', index: 'ReviewCategoryName',resizable:false, width: 250, sortable: false, formatter: function (cellvalue, options, rowobject) { return '<B>'+cellvalue+'</B>';}},
                    { name: 'KeycatNumber', index: 'KeycatNumber', sortable: false,resizable:false, width: 50,formatter: function (cellvalue, options, rowobject) {return '';} },
                    { name: 'KeycatName', index: 'KeycatName', sortable: false,resizable:false, width: 250 },
                    { name: 'Selected', index: 'Selected', sortable: false, width: 50,resizable:false, align: 'center', formatter:ReviewCategotySelected} ,
                    { name: 'QCUnderReq', index: 'QCUnderReq', sortable: false,  width: 50,resizable:false, align: 'center', formatter:ReviewCategotyQCUnderReq},
               		{ name: 'QCUnderPercent', index: 'QCUnderPercent', sortable: false,resizable:false, width: 50,align: 'center',formatter:ReviewCatPercent },
               		{ name: 'NumberOfWeeks', index: 'NumberOfWeeks',resizable:false, sortable: false,align: 'center',formatter:ReviewCatWeeks }
               	],
            viewrecords: true,
            multiselect: false,
            selectable: false,
            caption: "Categories / Keycats",
            rowNum: 2000,
            height: "300",
            gridview: true,
            rownumbers: false,
            width: grdWidth,
            autoencode: true,
            idPrefix: mainGridPrefix,
            loadonce: true,
            subGrid: true,
            subGridOptions: {
                "plusicon": "ui-icon-plus",
                "minusicon": "ui-icon-minus",
                "openicon": "ui-icon-arrowreturn-1-e",
                "reloadOnExpand": false,
                "selectOnExpand": true
            },
            subGridRowExpanded: function (subgridDivId, rowId) {
                var rowData = jQuery(this).getRowData(rowId);
                var reviewCategoryNumber = rowData['ReviewCategoryNumber'];
                var $subgrid = $("<table id='" + reviewCategoryNumber + "_t'></table>"),

                    pureRowId = $.jgrid.stripPref(mainGridPrefix, rowId),
                    colModel = $(this).jqGrid("getGridParam", "colModel");

                $subgrid.appendTo("#" + $.jgrid.jqID(subgridDivId));
                var keycatList = $.grep(objReviewCategoryDtls,function(n,i){
                    if(n.ReviewCategoryNumber == reviewCategoryNumber && parseInt(n.KeycatNumber)>0){
                        return n;
                    }
                });

                $subgrid.jqGrid({
                    datatype: 'local',
                    data: keycatList,
                    mtype: 'GET',
                    colNames: ['ReviewCatNumber', 'Review Category', 'Keycat', 'Keycat Name', 'Selected', 'QCUnder', 'Pass/Fail%', 'Weeks'],
                    colModel: [
                        { name: 'ReviewCategoryNumber', index: 'ReviewCategoryNumber', width: 50, sortable: false, hidden: true },
                        { name: 'ReviewCategoryName', index: 'ReviewCategoryName', resizable:false,width: 250, sortable: false},
                        { name: 'KeycatNumber', index: 'KeycatNumber', sortable: false,resizable:false, width: 50 },
                        { name: 'KeycatName', index: 'KeycatName', sortable: false,resizable:false, width: 250 },
                        { name: 'Selected', index: 'Selected', sortable: false, resizable:false,width: 50, align: 'center', formatter:KeycatSelected},
                        { name: 'QCUnderReq', index: 'QCUnderReq', sortable: false, resizable:false, width: 50, align: 'center', formatter:KeycatQCUnderReq},
               		    { name: 'QCUnderPercent', index: 'QCUnderPercent', sortable: false, resizable:false,width: 50,align: 'center',formatter:KeycatPercent },
               		    { name: 'NumberOfWeeks', index: 'NumberOfWeeks', resizable:false,sortable: false,align: 'center',formatter:KeycatWeeks }
               	    ],
                    rowNum: 1000,
                    width: grdWidth - 45,
                    height: "100%",
                    autoencode: true,
                    gridview: true,
                    loadonce: true,
                    beforeSelectRow: function (rowid, e) { return false; },
                    idPrefix: rowId + "_",
                    loadComplete: function (data) {
                          $('.keyCatPercent').bind('keypress', numbersonly);
                          $('.keyCatWeeks').bind('keypress', numbersonly);
                    }
                });
                //Remove the subgrid header
                $subgrid.closest("div.ui-jqgrid-view").children("div.ui-jqgrid-hdiv").hide();
            },
            resizeStop: function (newWidth, index) {
                var widthChange = this.newWidth - this.width,
                    $theGrid = $(this.bDiv).find(">div>.ui-jqgrid-btable"),
                    $subgrids = $theGrid.find(">tbody>.ui-subgrid>.subgrid-data>.tablediv>.ui-jqgrid>.ui-jqgrid-view>.ui-jqgrid-bdiv>div>.ui-jqgrid-btable");
                $subgrids.each(function () {
                    var grid = this.grid;
                    // we have subgrids which have no internal subgrids
                    // it reduce the number of columns to 1
                    // we have rownumbers: true in the main grid, but not in subgrids
                    // it reduce the number of columns to additional 1
                    // so we should use (index - 2) as the column index in the subgrids
                    grid.resizing = { idx: (index - 2) };
                    grid.headers[index - 2].newWidth = (index - 2 === 0) || (index - 1 === grid.headers.length) ? newWidth - 2 : newWidth;
                    grid.newWidth = grid.width + widthChange;
                    grid.dragEnd.call(grid);
                    $(this).jqGrid("setGridWidth", grid.newWidth, false);
                });
                $theGrid.jqGrid("setGridWidth", this.newWidth, false);
            }
        });

        //Review category grid
        jQuery("#grdSelectedCollCatgs").jqGrid({
            mtype: 'GET',
            data: objSelectedReviewCatDtls,
            datatype: 'local',
            async: false,
            colNames: ['SequenceNumber', 'Collection Category Name', 'Category Type', 'Additional Information', 'QC Under', 'Pass/Fail%', 'Weeks'],
            colModel: [
                    { name: 'SequenceNumber', index: 'SequenceNumber', width: 50, sortable: false, hidden: true },
                    { name: 'CategoryName', index: 'CategoryName',resizable:false, width: 350, sortable: false},
                    { name: 'CatgDetail', index: 'CatgDetail', sortable: false,resizable:false, width: 100},
                    { name: 'CategoryID', index: 'CategoryID', sortable: false,resizable:false, width: 100 },
                    { name: 'QCUnderReq', index: 'QCUnderReq', sortable: false,  width: 50,resizable:false, align: 'center', formatter: function (cellvalue, options, rowobject) { if (cellvalue == 1) { return '<input type="checkbox"  disabled="disabled" checked="true" />'; } else { return '<input type="checkbox" disabled="disabled" />' } } },
               		{ name: 'QCUnderPercent', index: 'QCUnderPercent', sortable: false,resizable:false, width: 50,align: 'center' },
               		{ name: 'NumberOfWeeks', index: 'NumberOfWeeks',resizable:false, sortable: false,align: 'center' }
               	],
            viewrecords: true,
            multiselect: false,
            selectable: false,
            caption: "Selected Categories",
            rowNum: 2000,
            height: "100",
            gridview: true,
            rownumbers: false,
            width: grdWidth-50,
            autoencode: true,
            loadonce: true
        });
        $('.chkRcatSelected').change(function(e){
            var selectedVal = e.currentTarget.checked?1:0
            var row = jQuery(e.target).closest('tr.jqgrow');
            var rowId = row.attr('id');
            var dataFromRow = jQuery('#grdCollCatDtls').jqGrid('getRowData', rowId);
            var obj = $.grep(objReviewCategoryDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber)
                    return n;
            });
             if(selectedVal == 1){
                 var isAllowed = CheckKeyCatOrReviewCatAlreadyExist(obj[0],RCAT);
                 if(!isAllowed){
                    alert("You can't select a Category when a KeyCat from it is selected");
                    e.preventDefault();
                    e.currentTarget.checked = false;
                    return false;
                 }
             }
            RemoveOrAddEntryInSelectedCategoryObject(selectedVal,obj[0],RCAT);
        });

         $('.chkRcatQCUnder').change(function(e){
            var selectedVal = e.currentTarget.checked?1:0
            var row = jQuery(e.target).closest('tr.jqgrow');
            var rowId = row.attr('id');
            var dataFromRow = jQuery('#grdCollCatDtls').jqGrid('getRowData', rowId);
            var obj = $.grep(objReviewCategoryDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber)
                    return n;
            });
            if(selectedVal == 1){
                var isAllowed = CheckKeyCatOrReviewCatAlreadyExist(obj[0],RCAT);
                if(!isAllowed){
                    alert("You can't select a Category when a KeyCat from it is selected");
                    e.preventDefault();
                    e.currentTarget.checked = false;
                    return false;
                }
             }
            AddOrUpdateEntryInSelectedCategoryObjectForQCUnder(selectedVal,obj[0],RCAT);
        });
       
         $('.reviewCatPercent').bind('keypress', numbersonly);
         $('.reviewCatWeeks').bind('keypress', numbersonly);
    });//End of document.ready

    function doShelfValidation() {
        if (parseInt($("#txtSoft").val(),10) < parseInt(($("#txtMin").val()),10)) {
            alert("'Soft Check' must be greater than or equal to the Min Limit")           
            return false;
        }

        var intVal = parseInt($("#txtSoft").val()) + 3;
        if (parseInt($("#txtHard").val(),10) < intVal) {
            alert("The value for 'Hard Check' must be atleast 3 greater than the value of 'Soft Check'");
            return false;
        }

        if (currActiveTab == 'trLocation') {
            if (parseInt($("#txtHard").val(),10) > parseInt(($("#txtMax").val()),10)) {
                alert("Max Limit must be greater than the Hard Check");
                return false;
            }
        }

        return true;
    }

    function resetTreeVal(ctlName) {
        var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
        var node;

        switch (currActiveTab) {
            case "trLocation":
                node = zTree1.getNodeByTId("trLocation_1");
                break;
            case "trSection":
                node = zTree1.getNodeByTId("trSection_1");
                break;
            case "trShelf":
                node = zTree1.getNodeByTId("trShelf_1");
                break;
            case "trUPC":
                node = zTree1.getNodeByTId("trUPC_1");
                break;
        }

        if (ctlName == 'txtSoft')
            $("#txtSoft").val(node.task_soft_check);

        if (ctlName == 'txtHard')
            $("#txtHard").val(node.task_hard_check);

        if (ctlName == 'txtMin')
            $("#txtMin").val(node.taskMin);

        if (ctlName == 'txtMax')
            $("#txtMax").val(node.taskMax);
    }

     function updateValuetoTree() {
        var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
            var node;

            switch (currActiveTab) {
                case "trLocation":
                    node = zTree1.getNodeByTId("trLocation_1");
                    break;
                case "trSection":
                    if(activeSectionMinMax == 15995) // For cooler
                    {
                    node = zTree1.getNodeByTId("trSection_64");
                    }
                    else if(activeSectionMinMax == 15490) // For coffin
                    {
                     node = zTree1.getNodeByTId("trSection_75");
                    }
                    else if (activeSectionMinMax == 11160) // For Section width under presence of section
                    {
                     node = zTree1.getNodeByTId("trSection_18");
                    }
                    else if (activeSectionMinMax == 14825) // For Section width under shelf
                    {
                     node = zTree1.getNodeByTId("trSection_49");
                    }
                    else if (activeSectionMinMax == 16910) // For Section depth
                    {
                     node = zTree1.getNodeByTId("trSection_151");
                    }
                    else
                     node = zTree1.getNodeByTId("trSection_1");
                    break;
                case "trShelf":
                        if (activeShelfMinMax == 22700)
                        {
                        node = zTree1.getNodeByTId("trShelf_36");
                        }
                     else{
                        node = zTree1.getNodeByTId("trShelf_1");
                        }
                    break;
                case "trUPC":
                    node = zTree1.getNodeByTId("trUPC_1");
                    break;
            }
           
            node.taskMin = $("#txtMin").val();
            node.taskMax = $("#txtMax").val();
            if(currActiveTab !='trShelf')
            {
            node.task_soft_check = $("#txtSoft").val();
            node.task_hard_check = $("#txtHard").val();
            }
        }

        function saveShelfData() {
            if(ValidateShelfData()){
                var taskShelfVarObj = new Array();
                populateShelfVarObject(taskShelfVarObj, 'trLocation', "trLocation_1");
                populateShelfVarObject(taskShelfVarObj, 'trSection', "trSection_1");
                populateShelfVarObject(taskShelfVarObj, 'trShelf', "trShelf_1");
                populateShelfVarObject(taskShelfVarObj, 'trUPC', "trUPC_1");
                var shelfVarData = JSON.stringify(taskShelfVarObj);
                $.ajax({
                    type: "POST",
                    data: { shelfVarData: shelfVarData,taskId:glbShelfTaskNum },
                    url: '@Url.Action("SaveShelfVarDetails", "Task")',
                    dataType: "json",
                    async: false,
                    traditional: true,
                    success: function (result) {
                        var rst = result;
                    },
                    error: function (result) {
                        alert("Error occured while saving shelf data.");
                    }
                });
                SaveReviewCategoryDetailsForShelf();
            }
        }

        function populateShelfVarObject(taskShelfVarObj, treeName, treeNodeTId) {
            var treeObj = $.fn.zTree.getZTreeObj(treeName);
            var node = treeObj.getNodeByTId(treeNodeTId);
            if (node != null) {
             
                populateSubNodes(taskShelfVarObj, node);
            }
        }
        function populateSubNodes(taskShelfVarObj, node) {
            if (node.children != null && node.children.length > 0) {
                if (node.checked) {
                    var shelfVar = { "task_number": glbShelfTaskNum,
                        "prompt_level": 0,
                        "ShelfVarID": node.id,
                        "min": node.taskMin,
                        "max": node.taskMax,
                        "soft_check": node.task_soft_check,
                        "hard_check": node.task_hard_check,
                        "ordinal": node.mainOrdinal,
                        "name": node.name
                    }
                    taskShelfVarObj.push(shelfVar);
                    for (var i = 0; i < node.children.length; i++) {
                        populateSubNodes(taskShelfVarObj, node.children[i]);
                    }
                }
               
            }
            else {
                if (node.checked) {
                    var shelfVar = { "task_number": glbShelfTaskNum,
                        "prompt_level": 0,
                        "ShelfVarID": node.id,
                        "min": node.taskMin,
                        "max": node.taskMax,
                        "soft_check": node.task_soft_check,
                        "hard_check": node.task_hard_check,
                        "ordinal": node.mainOrdinal,
                        "name":node.name
                    }
                    taskShelfVarObj.push(shelfVar);
                }
                return;
            }
        }

        //Sandeep Singh     29-Aug-2023
        function ValidateShelfData(){
            var treeObj = $.fn.zTree.getZTreeObj('trLocation');
            var node = treeObj.getNodeByTId('trLocation_1');
            var checkedLength = 0;

            for(var i = 0; i < node.children.length; i++)
            {
                if(node.children[i].checked){                    
                    checkedLength += 1;
                }
            }

            if(checkedLength == 0){
                alert("Please select at least one Location!");
                isGlobalError = true;
                return false;
            }
            else
                return true;
        }

  function ShelfMinValidation()
  {
      var minVal = GetMinMaxValueForShelf(1);
       if(parseInt($("#txtMin").val(),10) > minVal)
       {
        alert('The value for Min must be less than the smallest selected width variable');
        return false;
       }

       return true;
  }

  function ShelfMaxValidation()
  {
    var mxaVal = GetMinMaxValueForShelf(2);
    if($("#txtMax").val() =='')
    {
     alert('A values is required for Max');
     return  false;
    }
    else if(parseInt($("#txtMax").val(),10) < mxaVal)
    {
    alert('The value of Max must be greater than the largest selected width variable');
    return false;
    }

    return true;
  }

  function GetMinMaxValueForShelf(val)
  {
           var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
           var node = zTree1.getNodeByTId("trShelf_1");
           var arrVal = new Array();
           if (node.children != null && node.children.length > 0)
           {
             for(var i =0;i<node.children.length ;i++)
             {
               if(node.children.length >0)
               {
                for(var j=0;j<node.children[i].children.length ;j++)
                {
                 if(node.children[i].children[j].name == 'Height')
                 {
                  for(var k=0;k<node.children[i].children[j].children.length ;k++)
                  {
                   if(node.children[i].children[j].children[k].checked && node.children[i].children[j].children[k].name !='Other Height')
                   {
                    arrVal.push(node.children[i].children[j].children[k].value);
                   }
                  }
                 }
                }
               }
             }
           }
         var max_of_array = Math.max.apply(Math, arrVal);
         var min_of_array = Math.min.apply(Math, arrVal);
         if(val == 1)
         return min_of_array;
         else
         return max_of_array;
  }


  function SectionMinValidation()
  {
      var minVal = GetMinMaxValueOfSection(1);
       if(parseInt($("#txtMin").val(),10) > minVal && minVal != -1)
       {
        alert('The value for Min must be less than the smallest selected width variable');
        return false;
       }

       return true;
  }

  function SectionMaxValidation()
  {
    var mxaVal = GetMinMaxValueOfSection(2);
    if($("#txtMax").val() =='')
    {
     alert('A values is required for Max');
     return  false;
    }
    else if(parseInt($("#txtMax").val(),10) < mxaVal && mxaVal != -1 )
    {
    alert('The value of Max must be greater than the largest selected width variable');
    return false;
    }

    return true;
  }

  function GetMinMaxValueOfSection(val)
  {
      var zTree1 = $.fn.zTree.getZTreeObj(currActiveTab);
      var node;
      var max_of_array =-1;
      var min_of_array =-1;

      if(activeSectionMinMax == 11160)
       node = zTree1.getNodeByTId("trSection_3");
      else if(activeSectionMinMax == 15490)
        node = zTree1.getNodeByTId("trSection_66");
      else if(activeSectionMinMax == 15995)
         node = zTree1.getNodeByTId("trSection_53");
      else if(activeSectionMinMax == 14825)
         node = zTree1.getNodeByTId("trSection_38");
      else if(activeSectionMinMax == 16910)
         node = zTree1.getNodeByTId("trSection_135"); 

       var arrVal = new Array();
        if (node.children != null && node.children.length > 0)
         {
           for(var i =0;i<node.children.length ;i++)
             {
              if(node.children[i].checked && (node.children[i].name !='Other Section Width' && node.children[i].name !='Other Section Depth'))
              {
               arrVal.push(node.children[i].value);
              }
             }
         }
         if(arrVal == '' || arrVal.length ==0)
         {
         }
         else{
         max_of_array = Math.max.apply(Math, arrVal);
         min_of_array = Math.min.apply(Math, arrVal);
         }
         if(val == 1)
         return min_of_array;
         else
         return max_of_array;
  }
  function ReviewCategotySelected(cellvalue, options, rowobject){
     var disabled =(_taskVersion == 'FNL')? ' disabled="true"':' ';
     var id = 'chkRcatSelected_' + rowobject.ReviewCategoryNumber;  
     if (cellvalue == 1) { 
        return '<input id="' + id + '" '+disabled+' class="chkRcatSelected" type="checkbox" checked="true" />';
     } 
     else { 
        return '<input id="' + id + '" '+disabled+' class="chkRcatSelected" type="checkbox" />'; 
     }
  }
   function ReviewCategotyQCUnderReq(cellvalue, options, rowobject){
        
     var disabled =(_taskVersion == 'FNL')? ' disabled="true"':' ';
     var id = 'chkRcatQCUnder_' + rowobject.ReviewCategoryNumber; 
     if (cellvalue == 1) { 
        return '<input id="' + id + '" type="checkbox" '+disabled+' class="chkRcatQCUnder" checked="true" />';
     } 
     else { 
        return '<input type="checkbox" '+disabled+' class="chkRcatQCUnder" id="' + id + '"  />' ; 
     }
  }
  function ReviewCatPercent(cellvalue, options, rowobject){
        var disabled =(rowobject.QCUnderReq == null || rowobject.QCUnderReq == 0 || _taskVersion == 'FNL')? ' disabled="true"':' ';
        
        cellvalue = cellvalue==null?'':cellvalue;
        var id='txtRcatPercent_'+rowobject.ReviewCategoryNumber;
        if (_taskVersion == 'FNL')
            var QCPercent = "<input id='"+id+"' " + disabled + " class='reviewCatPercent' onchange=ReviewCatPercentOnChange(this,"+rowobject.ReviewCategoryNumber+")  style ='width:40px;font-size:1.0em' type='text'  maxlength='2' value='" + cellvalue + "' />";
        else
            var QCPercent = "<input id='"+id+"' " + disabled + " class='reviewCatPercent' style ='width:40px;font-size:1.0em' type='text' onchange=ReviewCatPercentOnChange(this,"+rowobject.ReviewCategoryNumber+")   maxlength='2' value='" + cellvalue + "' />";
        return QCPercent;
  }
  function ReviewCatWeeks(cellvalue, options, rowobject){
        var disabled =(rowobject.QCUnderReq == null || rowobject.QCUnderReq == 0 || _taskVersion == 'FNL')? ' disabled="true"':' ';
        cellvalue = cellvalue==null?'':cellvalue;
        var id='txtRcatWeeks_'+rowobject.ReviewCategoryNumber;
        if (_taskVersion == 'FNL')
            var QCWeeks = "<input id='"+id+"' " + disabled + " class='reviewCatWeeks' onchange=ReviewCatWeeksOnChange(this,"+rowobject.ReviewCategoryNumber+") style ='width:40px;font-size:1.0em' type='text'  maxlength='2' value='" + cellvalue + "' />";
        else
            var QCWeeks = "<input id='"+id+"' " + disabled + " class='reviewCatWeeks' onchange=ReviewCatWeeksOnChange(this,"+rowobject.ReviewCategoryNumber+") style ='width:40px;font-size:1.0em' type='text'  maxlength='2' value='" + cellvalue + "' />";
        return QCWeeks;
  }
  function KeycatPercent(cellvalue, options, rowobject){
        var disabled =(rowobject.QCUnderReq == null || rowobject.QCUnderReq == 0 || _taskVersion == 'FNL')? ' disabled="true"':' ';
        cellvalue = cellvalue==null?'':cellvalue;
        var id='txtKcatPercent_'+rowobject.ReviewCategoryNumber+'_'+rowobject.KeycatNumber;
        if (_taskVersion == 'FNL')
            var QCPercent = "<input id='"+id+"' " + disabled + " class='keyCatPercent' onchange=KeycatPercentOnChange(this,"+rowobject.ReviewCategoryNumber+","+rowobject.KeycatNumber+") style ='width:40px;font-size:1.0em' type='text'  maxlength='2' value='" + cellvalue + "' />";
        else
            var QCPercent = "<input id='"+id+"' " + disabled + " class='keyCatPercent' onchange=KeycatPercentOnChange(this,"+rowobject.ReviewCategoryNumber+","+rowobject.KeycatNumber+") style ='width:40px;font-size:1.0em' type='text'  maxlength='2' value='" + cellvalue + "' />";
        return QCPercent;
  }
  function KeycatWeeks(cellvalue, options, rowobject){
        
        var disabled = (rowobject.QCUnderReq == null || rowobject.QCUnderReq == 0 || _taskVersion == 'FNL')  ? ' disabled="true"':' ';
        cellvalue = cellvalue==null?'':cellvalue;
        var id='txtKcatWeeks_'+rowobject.ReviewCategoryNumber+'_'+rowobject.KeycatNumber;
        if (_taskVersion == 'FNL')
            var QCWeeks = "<input id='"+id+"' " + disabled + " class='keyCatWeeks' onchange=KeycatWeeksOnChange(this,"+rowobject.ReviewCategoryNumber+","+rowobject.KeycatNumber+") style ='width:40px;font-size:1.0em' type='text'  maxlength='2' value='" + cellvalue + "' />";
        else
            var QCWeeks = "<input id='"+id+"' " + disabled + " class='keyCatWeeks' onchange=KeycatWeeksOnChange(this,"+rowobject.ReviewCategoryNumber+","+rowobject.KeycatNumber+") style ='width:40px;font-size:1.0em' type='text'  maxlength='2' value='" + cellvalue + "' />";
        return QCWeeks;
  }
  function KeycatSelected(cellvalue, options, rowobject){
    var disabled = (_taskVersion == 'FNL')  ? ' disabled="true"':' ';
    var id = 'chkKcatSelected_' + rowobject.ReviewCategoryNumber+'_'+rowobject.KeycatNumber; 
    if(cellvalue == 1){ 
        return '<input id="' + id + '" class="chkKcatSelected" '+disabled+' type="checkbox" checked="true" onclick="KeycatSelectedOnChange(this,'+rowobject.ReviewCategoryNumber+','+rowobject.KeycatNumber+');" />'; 
    }
    else{
        return '<input id="' + id + '" class="chkKcatSelected" '+disabled+' type="checkbox" onclick="KeycatSelectedOnChange(this,'+rowobject.ReviewCategoryNumber+','+rowobject.KeycatNumber+');"  />'; 
    }
  }
  function KeycatQCUnderReq(cellvalue, options, rowobject){
    var disabled = (_taskVersion == 'FNL')  ? ' disabled="true"':' ';
    var id = 'chkKcatQCUnder_' + rowobject.ReviewCategoryNumber+'_'+rowobject.KeycatNumber;  
    if (cellvalue == 1) { 
        return '<input id="' + id + '" class="chkKcatQCUnder" '+disabled+' type="checkbox" checked="true" onclick="KeycatQCUnderOnChange(this,'+rowobject.ReviewCategoryNumber+','+rowobject.KeycatNumber+');" />'; 
    } 
    else {
        return '<input type="checkbox" id="' + id + '"  '+disabled+' onclick="KeycatQCUnderOnChange(this,'+rowobject.ReviewCategoryNumber+','+rowobject.KeycatNumber+');"  />' 
    } 
  }
  function ReviewCatPercentOnChange(txtObj,rCatNumber){
     //first find the object
     
     var obj = $.grep(objSelectedReviewCatDtls,function(n,i){
        if(n.ReviewCategoryNumber == rCatNumber && (n.Catg_Type!=null && n.Catg_Type=='R')){
            return n;
        }
     });
     if(obj!=null){
        obj[0].QCUnderPercent = $.trim($(txtObj).val())==''?null:$(txtObj).val();
        ReloadSelectedCollCatgGrid();
     }
  }
  function ReviewCatWeeksOnChange(txtObj,rCatNumber){
     //first find the object
     
     var obj = $.grep(objSelectedReviewCatDtls,function(n,i){
        if(n.ReviewCategoryNumber == rCatNumber && (n.Catg_Type!=null && n.Catg_Type=='R')){
            return n;
        }
     });
     if(obj!=null){
        obj[0].NumberOfWeeks = $.trim($(txtObj).val())==''?null:$(txtObj).val();
        ReloadSelectedCollCatgGrid();
     }
  }
  function KeycatSelectedOnChange(obj,rCatNumber,KeycatNumber){
    
     var selectedVal = obj.checked?1:0;
     var dataFromRow = $.grep(objReviewCategoryDtls,function(n,i){
        if(n.ReviewCategoryNumber == rCatNumber && n.KeycatNumber == KeycatNumber){
            return n;
        }
     });
     if(selectedVal == 1){
         var isAllowed = CheckKeyCatOrReviewCatAlreadyExist(dataFromRow[0],KCAT);
         if(!isAllowed){
            alert("You can't select a KeyCat when its Category is selected");
            obj.checked = false;
            return false;
         }
     }
     RemoveOrAddEntryInSelectedCategoryObject(selectedVal,dataFromRow[0],KCAT);
     return true;
  }
  function KeycatQCUnderOnChange(obj,reviewCategoryNumber,keycatNumber){
        
        var selectedVal = obj.checked?1:0
        var dataFromRow = $.grep(objReviewCategoryDtls,function(n,i){
            if(n.ReviewCategoryNumber == reviewCategoryNumber && n.KeycatNumber == keycatNumber){
                return n;
            }
        });
        if(selectedVal == 1){
            var isAllowed = CheckKeyCatOrReviewCatAlreadyExist(dataFromRow[0],KCAT);
            if(!isAllowed){
                alert("You can't select a KeyCat when its Category is selected");
                obj.checked = false;
                return false;
            }
         }
        AddOrUpdateEntryInSelectedCategoryObjectForQCUnder(selectedVal,dataFromRow[0],KCAT);
        return false;
  }
  function KeycatPercentOnChange(txtObj,rCatNumber,kCatNumber){
     //first find the object
     
     var obj = $.grep(objSelectedReviewCatDtls,function(n,i){
        if(n.ReviewCategoryNumber == rCatNumber && n.KeycatNumber == kCatNumber){
            return n;
        }
     });
     if(obj!=null){
        obj[0].QCUnderPercent = $.trim($(txtObj).val())==''?null:$(txtObj).val();
        ReloadSelectedCollCatgGrid();
     }
  }
  function KeycatWeeksOnChange(txtObj,rCatNumber,kCatNumber){
     //first find the object
     
     var obj = $.grep(objSelectedReviewCatDtls,function(n,i){
        if(n.ReviewCategoryNumber == rCatNumber && n.KeycatNumber == kCatNumber){
            return n;
        }
     });
     if(obj!=null){
        obj[0].NumberOfWeeks = $.trim($(txtObj).val())==''?null:$(txtObj).val();
        ReloadSelectedCollCatgGrid();
     }
  }
  function RemoveOrAddEntryInSelectedCategoryObject(selectedVal, dataFromRow,RcatKeyCat){
        var index = -1;
        var isRecordExist = null;
        var categoryID = null,name='',catDtl = '';
        if(RcatKeyCat == RCAT){
            isRecordExist = $.grep(objSelectedReviewCatDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber && (n.Catg_Type!=null && n.Catg_Type=='R')){
                    index = i;
                    return n;
                } 
            });
            categoryID = dataFromRow.ReviewCategoryNumber;
            name = dataFromRow.ReviewCategoryName;
            catDtl='Review Category';
        }
        else{
              isRecordExist = $.grep(objSelectedReviewCatDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber && n.KeycatNumber ==  dataFromRow.KeycatNumber){
                    index = i;
                    return n;
                } 
            });
            categoryID = dataFromRow.KeycatNumber;
            name = dataFromRow.KeycatName;
            catDtl='KeyCat';
        }
        if(selectedVal == 1){
        //Check the record is already exist for the selected review category
            if(index==-1){
                //insert the record
                var reviewCatObj = {"ReviewCategoryNumber":dataFromRow.ReviewCategoryNumber,
                "SequenceNumber":0,
                "TaskNumber":dataFromRow.TaskNumber,
                "Catg_Type":dataFromRow.Catg_Type,
                "Stub_Name":dataFromRow.Stub_Name,
                "Stub_Type":dataFromRow.Stub_Type,
                "KeycatNumber":dataFromRow.KeycatNumber,
                "CategoryID":categoryID,
                "CategoryName":name,
                "CatgDetail":catDtl,
                //"Selected":dataFromRow.Selected,
                "QCUnderReq":dataFromRow.QCUnderReq,
                "QCUnderPercent":dataFromRow.QCUnderPercent,
                "NumberOfWeeks":dataFromRow.NumberOfWeeks,
                "TranslationTask":dataFromRow.TranslationTask
                }
                objSelectedReviewCatDtls.push(reviewCatObj);
            }
        }
        else{
            if(index!=-1){
                objSelectedReviewCatDtls.splice(index,1);
                //Clear the control values of Selected, QCUnderReq, Pass/Fail% and weeks.
                if(RcatKeyCat == RCAT){
                    $('#chkRcatSelected_'+dataFromRow.ReviewCategoryNumber).prop('checked',false);
                    $('#chkRcatQCUnder_'+dataFromRow.ReviewCategoryNumber).prop('checked',false);
                    $('#txtRcatPercent_'+dataFromRow.ReviewCategoryNumber).val('');
                    $('#txtRcatWeeks_'+dataFromRow.ReviewCategoryNumber).val('');
                    $('#txtRcatPercent_'+dataFromRow.ReviewCategoryNumber).prop('disabled',true);
                    $('#txtRcatWeeks_'+dataFromRow.ReviewCategoryNumber).prop('disabled',true)
                }
                else{
                    $('#chkKcatSelected_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('checked',false);
                    $('#chkKcatQCUnder_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('checked',false);
                    $('#txtKcatPercent_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).val('');
                    $('#txtKcatWeeks_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).val('');
                    $('#txtKcatPercent_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('disabled',true);
                    $('#txtKcatWeeks_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('disabled',true);
                }
            }
        }
        ReloadSelectedCollCatgGrid();
  }

   function AddOrUpdateEntryInSelectedCategoryObjectForQCUnder(selectedVal, dataFromRow,RcatKeyCat){
        var index = -1;
        var isRecordExist = null;
        var categoryID = null,name='',catDtl = '';
        if(RcatKeyCat == RCAT){
            isRecordExist = $.grep(objSelectedReviewCatDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber && (n.Catg_Type!=null && n.Catg_Type=='R')){
                    index = i;
                    return n;
                } 
            });
            categoryID = dataFromRow.ReviewCategoryNumber;
            name = dataFromRow.ReviewCategoryName;
            catDtl='Review Category';
        }
        else{
              isRecordExist = $.grep(objSelectedReviewCatDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber && n.KeycatNumber ==  dataFromRow.KeycatNumber){
                    index = i;
                    return n;
                } 
            });
            categoryID = dataFromRow.KeycatNumber;
            name = dataFromRow.KeycatName;
            catDtl='KeyCat';
        }
        if(selectedVal == 1){
        //Check the record is already exist for the selected review category if not exist create the entry
            var defaultNumberOfWeeks = 5;
            if(index==-1){
                //insert the record
                var reviewCatObj = {"ReviewCategoryNumber":dataFromRow.ReviewCategoryNumber,
                "SequenceNumber":0,
                "TaskNumber":dataFromRow.TaskNumber,
                "Catg_Type":dataFromRow.Catg_Type,
                "Stub_Name":dataFromRow.Stub_Name,
                "Stub_Type":dataFromRow.Stub_Type,
                "KeycatNumber":dataFromRow.KeycatNumber,
                "CategoryID":categoryID,
                "CategoryName":name,
                "CatgDetail":catDtl,
               // "Selected":1,
                "QCUnderReq":1,
                "QCUnderPercent":dataFromRow.QCUnderPercent,
                "NumberOfWeeks":defaultNumberOfWeeks,
                "TranslationTask":dataFromRow.TranslationTask
                }
                objSelectedReviewCatDtls.push(reviewCatObj);
            }
            else{
                objSelectedReviewCatDtls[index].QCUnderReq = 1;
                objSelectedReviewCatDtls[index].NumberOfWeeks = defaultNumberOfWeeks;
            }
            if(RcatKeyCat == RCAT){
                $('#txtRcatWeeks_'+dataFromRow.ReviewCategoryNumber).val(defaultNumberOfWeeks);
                $('#chkRcatSelected_'+dataFromRow.ReviewCategoryNumber).prop('checked',true);
                $('#txtRcatPercent_'+dataFromRow.ReviewCategoryNumber).prop('disabled',false);
                $('#txtRcatWeeks_'+dataFromRow.ReviewCategoryNumber).prop('disabled',false)
            }
            else{
                $('#txtKcatWeeks_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).val(defaultNumberOfWeeks);
                $('#chkKcatSelected_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('checked',true);
                $('#txtKcatPercent_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('disabled',false);
                $('#txtKcatWeeks_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('disabled',false);
            }
        }
        else{
            //Clear the control values of Selected, QCUnderReq, Pass/Fail% and weeks.
            if(RcatKeyCat == RCAT){
                $('#chkRcatQCUnder_'+dataFromRow.ReviewCategoryNumber).prop('checked',false);
                $('#txtRcatPercent_'+dataFromRow.ReviewCategoryNumber).val('');
                $('#txtRcatWeeks_'+dataFromRow.ReviewCategoryNumber).val('');
                $('#txtRcatPercent_'+dataFromRow.ReviewCategoryNumber).prop('disabled',true);
                $('#txtRcatWeeks_'+dataFromRow.ReviewCategoryNumber).prop('disabled',true)
             }
            else{
                $('#chkKcatQCUnder_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('checked',false);
                $('#txtKcatPercent_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).val('');
                $('#txtKcatWeeks_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).val('');
                $('#txtKcatPercent_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('disabled',true);
                $('#txtKcatWeeks_'+dataFromRow.ReviewCategoryNumber+'_'+dataFromRow.KeycatNumber).prop('disabled',true);
            }
            if(index!=-1){
                objSelectedReviewCatDtls[index].QCUnderReq = 0;
                objSelectedReviewCatDtls[index].NumberOfWeeks = null;
                objSelectedReviewCatDtls[index].QCUnderPercent = null;
            }
        }
        ReloadSelectedCollCatgGrid();
  }
  function CheckKeyCatOrReviewCatAlreadyExist(dataFromRow,RcatKeyCat){
        
        var index = -1;
        var isRecordExist = null;
        //Check any keycats already selected? then do not allow to select review category
        if(RcatKeyCat == RCAT){
            isRecordExist = $.grep(objSelectedReviewCatDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber && parseInt(n.KeycatNumber)>0){
                    index = i;
                    return n;
                } 
            });
        }
        else{
            //For keycat check if any review category is selected
              isRecordExist = $.grep(objSelectedReviewCatDtls,function(n,i){
                if(n.ReviewCategoryNumber == dataFromRow.ReviewCategoryNumber && (n.Catg_Type!=null && n.Catg_Type=='R')){
                    index = i;
                    return n;
                } 
            });
        }
        //if index is >0 then do not allow it to select the keycat or review category
        var allowed = index!=-1?false:true;
        return allowed;
  }
  function ReloadSelectedCollCatgGrid(){
         //Reset question grid wtih modified data
        $("#grdSelectedCollCatgs").setGridParam({
            datastr: objSelectedReviewCatDtls,
            datatype: "jsonstring"
        }).trigger("reloadGrid");
  }
  function SaveReviewCategoryDetailsForShelf(){
        
        var reviewCats = JSON.stringify(objSelectedReviewCatDtls);
        $.ajax({
            type: "POST",
            data: { reviewCats: reviewCats,task_number:glbShelfTaskNum },
            url: '@Url.Action("SaveReviewCategoryDetailsForShelf", "Task")',
            dataType: "json",
            async: false,
            traditional: true,
            success: function (result) {
                var rst = result;
            },
            error: function (result) {
                alert("Error occured while saving collection categories data for shelf.");
            }
        });
  }
</script>
